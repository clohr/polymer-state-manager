{
  "extends": ["eslint:recommended", "google", "es2015"],
  "env": {
    "browser": true
  },
  "plugins": [
    "html"
  ],
  "rules": {
    "no-var": "off",
    "semi": "off",
    "no-undef": "off",
    "max-len": 0,
    "new-cap": ["error", { "capIsNewExceptions": ["Polymer"] }]
  },
  "globals": {
    "Polymer": true
  }
}
